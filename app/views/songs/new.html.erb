<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.text_field :title %>
  <br />
  
  <%= f.text_field :artist_name %>
  
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  
  <input name='song[note_attributes_content][]' id='song_notes_attributes_content'>

  <%= f.submit %>
<% end %>


